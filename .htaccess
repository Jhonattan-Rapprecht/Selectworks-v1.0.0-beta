RewriteEngine On

# Do NOT rewrite static assets
RewriteRule ^slw-webapp-v1/app-includes/style/css/ - [L]
RewriteRule ^slw-webapp-v1/app-includes/style/js/ - [L]
RewriteRule ^slw-webapp-v1/app-includes/style/images/ - [L]

# Do NOT rewrite any file that actually exists
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule ^ - [L]

# Do NOT rewrite directories
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# Rewrite everything else to index.php
RewriteRule ^ index.php [L]


