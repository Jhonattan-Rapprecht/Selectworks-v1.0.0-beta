{
  "name": "Selectworks-Codespace-Build-1.0.0-Dev",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "web",
  "workspaceFolder": "/workspace",
  "settings": {
    "php.validate.executablePath": "/usr/local/bin/php",
    "editor.formatOnSave": true
  },
  "extensions": [
    "bmewburn.vscode-intelephense-client",
    "xdebug.php-debug",
    "ms-azuretools.vscode-docker"
  ],
  "remoteUser": "root"
}
